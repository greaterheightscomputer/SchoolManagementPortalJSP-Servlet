	<parameter name="studentId" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
     assessments.`iTest1` AS assessments_iTest1,
     assessments.`iTest2` AS assessments_iTest2,
     assessments.`iExam` AS assessments_iExam,
     assessments.`iTotal` AS assessments_iTotal,
     assessments.`vRemark` AS assessments_vRemark,
     students.`cStudentId` AS students_cStudentId,
     concat(students.vFirstName, ' ' ,students.vLastName, ' ', students.vOtherNames) AS students_vFirstName,
     students.`cGender` AS students_cGender,
     students.`dDateOfBirth` AS students_dDateOfBirth,
     students.`dAdmissionDate` AS students_dAdmissionDate,
     students.`vParentName` AS students_vParentName,
     classes.`vClassName` AS classes_vClassName,
     classes.`vDescription` AS classes_vDescription,
     terms.`vTermName` AS terms_vTermName,
     subjects.`vSubjectName` AS subjects_vSubjectName,
     sessions.`vSessionName` AS sessions_vSessionName,
     sports.`vEvents` AS sports_vEvents,
     sports.`vLevelAttained` AS sports_vLevelAttained,
     sports.`vComments` AS sports_vComments,
     developments.`vHieghtBegin` AS developments_vHieghtBegin,
     developments.`vHieghtEnd` AS developments_vHieghtEnd,
     developments.`vWeightBegin` AS developments_vWeightBegin,
     developments.`vWeightEnd` AS developments_vWeightEnd,
     developments.`vDaysAbsent` AS developments_vDaysAbsent,
     developments.`VNature` AS developments_VNature,
     developments.`vOrganization` AS developments_vOrganization,
     developments.`vOfficeHeld` AS developments_vOfficeHeld,
     developments.`vContribution` AS developments_vContribution,
     developments.`vPrincipal` AS developments_vPrincipal,
     developments.`vTeacher` AS developments_vTeacher,
     developments.`dVacation` AS developments_dVacation,
     developments.`dResumption` AS developments_dResumption,
     ctimes.`vTimePresentinSchool` AS ctimes_vTimePresentinSchool,
     ctimes.`vTimeAbsentinSchool` AS ctimes_vTimeAbsentinSchool,
     ctimes.`vTimePresentinSport` AS ctimes_vTimePresentinSport,
     ctimes.`vTimeAbsentinSport` AS ctimes_vTimeAbsentinSport,
     ctimes.`vGreenN0` AS ctimes_vGreenN0,
     ctimes.`vGreenDeed` AS ctimes_vGreenDeed,
     ctimes.`vRedN0` AS ctimes_vRedN0,
     ctimes.`vRedDeed` AS ctimes_vRedDeed,
     ctimes.`vComments` AS ctimes_vComments,
     ctimes.`vCleanlinessRating` AS ctimes_vCleanlinessRating,
     ctimes.`vRemarks` AS ctimes_vRemarks
FROM
     `assessments` assessments INNER JOIN
     `students` students ON assessments.cStudentId = students.cStudentId INNER JOIN
     `classes` classes ON assessments.cClassId = classes.cClassId INNER JOIN
     `ctimes` ctimes ON assessments.cCtimeId = ctimes.cCtimeId INNER JOIN
     `developments` developments ON assessments.cDevelopmentId = developments.cDevelopmentId INNER JOIN
     `sessions` sessions ON assessments.cSessionId = sessions.cSessionId INNER JOIN
     `sports` sports ON assessments.cSportId = sports.cSportId INNER JOIN
     `subjects` subjects ON assessments.cSubjectId = subjects.cSubjectId INNER JOIN
     `terms` terms ON assessments.cTermId = terms.cTermId WHERE assessments.cStudentId=$P{studentId}]]>
	</queryString>
